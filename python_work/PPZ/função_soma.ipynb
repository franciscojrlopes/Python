   "source": [
    "def soma(*numeros):\n",
    "    resultado = 0\n",
    "    for num in numeros:\n",
    "        resultado += num\n",
    "    return resultado\n",
    "\n",
    "x = soma(2, 3, 4, 7)\n",
    "\n",
    "print(x)\n",
    "\n"
   ]
  }
